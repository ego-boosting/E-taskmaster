<h1>Mypage</h1>
<table>
  <thead>
    <tr>
      <th>名前</th>
      <th>メールアドレス</th>
      <th>登録日</th>
      <th colspan=2></th>
    </tr>
  </thead>
  <tbody>
    <tr>
      <td><%= @user.name %></td>
      <td><%= @user.email %></td>
      <td><%= @user.created_at.strftime('%Y/%m/%d %H:%M') %></td>
      <td><%= link_to "編集", edit_note_path(@user) %></td>
    </tr>
  </tbody>
</table>
