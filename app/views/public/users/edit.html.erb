  <h1>ユーザ編集</h1>
  <%= form_with model: @user, local: true do |f| %>
    <table>
      <tr>
        <th><%= f.label :"名前"%></th>
        <td><%= f.text_field :name %></td>
      </tr>
      <tr>
        <th><%= f.label :"メールアドレス"%></th>
        <td><%= f.text_field :email %></td>
      </tr>
    </table>
    <%= f.submit :"update" %>
  <% end %>
